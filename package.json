{
  "name": "botbuilder-slack",
  "version": "1.1.1",
  "description": "Slack Connector for botbuilder. Compatibe with Microsoft Slack connector.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/suttna/botbuilder-slack",
  "author": "Martin Fernandez <fmartin91@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest --config jest.json --forceExit --runInBand --verbose",
    "posttest": "codecov -f coverage/*.json",
    "build": "yarn build:src",
    "build:src": "tsc",
    "build:tests": "tsc -p __tests__/tsconfig.json",
    "docs": "typedoc --out docs lib/index.d.ts --mode file --gaID UA-102232188-2 --hideGenerator --readme none --name 'Botbuilder Slack' && touch docs/.nojekyll",
    "watch": "tsc --watch",
    "lint": "tslint 'src/**/*.ts' '__tests__/**/*.ts'",
    "lint:fix": "tslint 'src/**/*.ts' '__tests__/**/*.ts' --fix",
    "ci": "yarn lint && yarn build:src && yarn build:tests && yarn test",
    "prepare": "yarn build",
    "preversion": "yarn test",
    "changelog": "bundle exec github_changelog_generator"
  },
  "keywords": [
    "bot",
    "node",
    "botframework",
    "botbuilder",
    "slack",
    "connector"
  ],
  "dependencies": {
    "@slack/client": "^3.13.0",
    "botbuilder": "^3.9.1"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.19",
    "@types/jest": "22.0.1",
    "@types/nock": "9.1.1",
    "@types/node": "8.5.8",
    "@types/qs": "6.5.1",
    "@types/ws": "4.0.0",
    "codecov": "3.0.0",
    "jest": "22.0.6",
    "nock": "9.1.6",
    "ts-jest": "22.0.1",
    "tslint": "5.9.1",
    "typedoc": "0.9.0",
    "typescript": "2.6.2"
  }
}
